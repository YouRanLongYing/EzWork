<?xml version="1.0" encoding="utf-8" ?>
<memcached>
  <services>
    <add key="v1" address="192.168.35.70"  weight="1" port="11211" />
    <add key="v1" address="192.168.35.72" weight="1" port="11211" />
    <add key="v3" address="192.168.35.69" weight="1" port="11211" />
    <add key="v4" address="192.168.35.100" weight="1" port="11211" />
  </services>
  <initconns>3</initconns><!--初始连接数-->
  <minconns>3</minconns><!--最小连接数-->
  <maxconns>5</maxconns><!--最大连接数-->
  <connect_timeout>3000</connect_timeout><!--连接超时时间-->
  <socket_timeout>3000</socket_timeout><!--通讯的超时时间,设置为3秒（单位ms）.NET版本没有实现-->
  <maintenance_sleep>30</maintenance_sleep><!--维护线程的间隔激活时间，下面设置为60秒（单位s），设置为0表示不启用维护线程-->
  <failover>true</failover><!--故障切换-->
  <nagle>false</nagle><!--是否对TCP/IP通讯使用Nalgle算法，.NET版本没有实现-->
  <enable_compression>true</enable_compression>
  <hashing_algorithm>NewCompatibleHash</hashing_algorithm> <!--Native,OldCompatibleHash,NewCompatibleHash-->
  <maxidle>2160000</maxidle><!--连接的最大空闲时间，下面设置为6个小时（单位ms），超过这个设置时间，连接会被释放掉-->
  <max_busy>10000</max_busy><!--socket单次任务的最大时间，超过这个时间socket会被强行中断掉（当前任务失败）-->
</memcached>