    <script src="@Url.Content("~/Scripts/lib/xAdapter.js")" type="text/javascript"></script>
    <script type="text/javascript">window["cur__xdialogid__"] = "@xdialogid";</script>
    @RenderSection("CssScript",false)
</head>
<body style="@ViewBag.Layout_Body_CssText">
    @RenderBody()
</body>
</html>
