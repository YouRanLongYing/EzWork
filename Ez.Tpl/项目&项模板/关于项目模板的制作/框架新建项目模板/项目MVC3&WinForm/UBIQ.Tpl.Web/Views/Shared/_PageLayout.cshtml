<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    @{var xdialogid = Request["xdialogid"];}
    <link   href="@Url.Content("~/Content/css/font-awesome.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/lib/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/" + ViewBag.LayoutAction + "/" + ViewBag.UIStyle + "/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/" + ViewBag.LayoutAction + "/" + ViewBag.UIStyle + "/page.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/lib/jqgrid/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/lib/jqgrid/jqgrid-ui-custom-style02.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/lib/jquery.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/lib/ajaxhelper.jquery.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/lib/jquery.validate.js")" type="text/javascript"></script>
